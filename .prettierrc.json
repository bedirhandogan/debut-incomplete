{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 120,
  "arrowParens": "always",
  "bracketSpacing": true,

  "importOrder": ["<THIRD_PARTY_MODULES>", "^components/(.*)$", "^views/(.*)$", "^store/(.*)$", "^db/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
